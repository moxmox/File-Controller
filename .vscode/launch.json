    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/filecon.js",
            "args": [
                "ip",
                "192.168.0.11",
                "port",
                "3000"
            ]
        },

        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://12.168.0.11:80",
            "webRoot": "${workspaceFolder}"
        }
    ]
}