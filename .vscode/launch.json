    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "node_debug",
            "address": "192.168.0.11",
            "port": 80,
            "runtimeArgs": [
                "--debug=5858"
            ],
            "program": "${workspaceFolder}/filecon.js",
            "protocol": "legacy"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://192.168.0.11:80",
            "webRoot": "${workspaceFolder}"
        }
    ]
}